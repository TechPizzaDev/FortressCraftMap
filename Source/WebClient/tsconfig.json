{
	"compilerOptions": {
		"lib": [ "es2016", "dom" ],
		"target": "es6",
		"module": "es6",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"downlevelIteration": true,
		"sourceMap": true,
		"outDir": "../../WebRoot/js"
	},
	"files": [
		"Core/Constants.ts",
		"Helpers/EventEmitter.ts",
		"Graphics/RendererBase.ts"
	],
	"include": [
		"**/*.ts"
	]
}