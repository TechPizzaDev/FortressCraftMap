{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES2018",
		"lib": [ "ES2018", "DOM" ],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"downlevelIteration": true,
		"noFallthroughCasesInSwitch": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"outDir": "Public/"
	},
	"include": [
		"Source/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}