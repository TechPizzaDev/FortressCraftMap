{
	"compilerOptions": {
		"lib": [ "es2016", "dom" ],
		"target": "es6",
		"module": "es6",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"downlevelIteration": true,
		"sourceMap": true,
		"outDir": "Public/"
	},
	"include": [
		"Source/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}